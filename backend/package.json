{
  "name": "intervee-backend",
  "version": "1.0.0",
  "type": "module",
  "description": "INTERVEE Backend - Philippine OSH Practitioner Interview Assistant",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx src/seed.ts",
    "audit": "tsx audit/run-audit.ts",
    "audit:verify": "tsx audit/verify-knowledge-base.ts",
    "audit:all": "npm run audit:verify && npm run audit"
  },
  "dependencies": {
    "@prisma/client": "^5.10.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.0",
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "openai": "^4.28.0",
    "socket.io": "^4.7.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "@types/uuid": "^9.0.8",
    "prisma": "^5.10.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
